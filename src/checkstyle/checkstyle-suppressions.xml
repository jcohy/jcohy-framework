<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
		"-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
		"https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
	<suppress files=".+Configuration\.java" checks="HideUtilityClassConstructor" />
	<suppress files=".+Application\.java" checks="HideUtilityClassConstructor" />
	<suppress files=".+(Utils|Util|Converter|Helper|Properties|Prop|Filter|Dto|Excel|Builder).java$" checks="MagicNumberCheck" />
	<suppress files="[\\/]jcohy-framework-configuration-processor[\\/]" checks="AbstractClassNameCheck" />
	<suppress files="[\\/]jcohy-framework-utils[\\/]" checks="MagicNumberCheck" />
	<suppress files="[\\/]src[\\/]test[\\/]java[\\/]" checks="MagicNumberCheck" />

</suppressions>